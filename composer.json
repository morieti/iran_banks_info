{
    "name": "morieti/iran-banks-info",
    "description": "This package provides information about Iranian banks, such as iban code, card numbers and logos",
    "keywords": [
        "iran",
        "bank",
        "iban",
        "card number",
        "logo"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Morteza Pouretemadi",
            "email": "pouretemadi.morteza@gmail.com",
            "homepage": "https://morteza-pouretemadi.com"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Morieti\\IranBanksInfo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Morieti\\Tests\\IranBanksInfo\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
    },
    "scripts": {
        "post-autoload-dump": "Morieti\\IranBanksInfo\\Config::publishBankImages"
    }
}
